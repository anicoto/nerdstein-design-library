<div class="blog-tag-menu">
    {% include "@molecules/blog-tags.twig" with {"tag_list": tag_list_menu} %}
</div>
<div class="blog-list">
    {% for key,value in blog_list %}
        {% include "@molecules/list-element.twig" with {
            "title": value.title,
            "link_url": value.link_url,
            "body": value.body,
            "tag_list": value.tag_list,
            "posted_date": value.posted_date,
            "image_url": value.image_url
        } %}
    {% endfor %}
</div>